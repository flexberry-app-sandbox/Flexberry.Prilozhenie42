<h3 class="ui header">{{t "forms.i-i-s-prilozhenie-42-расп-прих-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.урок
      class=(if (v-get validationModel "урок" "isInvalid") "error")
      label=(t "forms.i-i-s-prilozhenie-42-расп-прих-e.урок-caption")
      data-test-i-i-s-prilozhenie-42-расп-прих-e-урок=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "урок" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-prilozhenie-42-расп-прих-e.периодНачала-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.периодНачала
      class=(if (v-get validationModel "периодНачала" "isInvalid") "error")
      data-test-i-i-s-prilozhenie-42-расп-прих-e-периодНачала=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "периодНачала" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-prilozhenie-42-расп-прих-e.периодКонца-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.периодКонца
      class=(if (v-get validationModel "периодКонца" "isInvalid") "error")
      data-test-i-i-s-prilozhenie-42-расп-прих-e-периодКонца=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "периодКонца" "message")}}
  </div>
</form>
